doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title=`${shortTitle} | TrainQuery`
    meta(name="robots", content="noindex")
    meta(name="theme-color", content="#000000")
    link(rel="shortcut icon", href="/favicon.svg")
  body
    style.
      @font-face {
        font-family: "Poppins";
        font-weight: 400;
        src: url(data:application/octet-stream;base64,);
      }
      @font-face {
        font-family: "Poppins";
        font-weight: 700;
        src: url(data:application/octet-stream;base64,);
      }
      * {
        font-family: "Poppins";
        font-weight: normal;
        font-size: 1rem;
        margin: 0;
        padding: 0;
      }
      :root {
        --color-paper-30: rgb(36, 41, 51);
        --color-ink-30: rgba(184, 207, 255, 0.3);
        --color-ink-80: rgba(235, 241, 255, 0.8);
        --color-ink-100: rgb(255, 255, 255);
        --color-accent: rgb(25, 214, 255);
      }
      html, body {
        height: 98%;
        height: 98svh;
      }
      body {
        background-color: var(--color-paper-30);
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        min-width: 18rem;
        min-height: 25rem;
      }
      main {
        display: flex;
        flex-direction: column;
        align-items: center;
        max-width: 24rem;
        padding: 1.5rem;
      }
      main > svg {
        color: var(--color-ink-30);
        margin-bottom: 2rem;
        width: 6rem;
        height: auto;
      }
      h1 {
        color: var(--color-ink-100);
        font-size: 1.5rem;
        font-weight: bold;
        text-align: center;
        margin-bottom: 0.5rem;
      }
      main > p {
        color: var(--color-ink-80);
        text-align: center;
      }
      main > p:not(:last-child) {
        margin-bottom: 2rem;
      }
      a {
        display: flex;
        align-items: center;
        user-select: auto;
        -webkit-user-select: auto;
        text-decoration: none;
        outline: none;
        gap: 0.5rem;
      }
      a:not(:last-child) {
        margin-bottom: 0.5rem;
      }
      a > p, a > svg {
        color: var(--color-accent);
      }
      a > svg {
        font-size: 1.2rem;
      }
      a:hover > p, a:focus-visible > p {
        text-decoration: underline var(--color-accent) 1px;
      }

