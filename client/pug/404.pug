extends page-template/page-template.pug

include iconify.pug

block head
  +head-content(
    "Not found",
    "/css/404.css",
    null,
    null,
    null)

block body
  .page-centered
    include not-found-svg.pug
    +not-found-svg(3)#banner-img(aria-hidden="true")
    h1 Error 404 - Page not found
    p This page doesn't exist, at least not anymore!
    a(href="javascript:history.back();")
      +iconify("uil:angle-left-b")
      p Go back
    a(href="/")
      +iconify("heroicons-outline:home")
      p Go to the homepage
